            "blockIdx": 27
        },
        {
            "jpName": "マルゼンスキー",
            "enName": "Maruzensky",
            "jpText": "でしょでしょー！",
            "enText": "I know!",
            "nextBlock": 29,
            "pathId": -6491721282931323361,
            "blockIdx": 28
        },
        {
            "jpName": "マルゼンスキー",
            "enName": "Maruzensky",
